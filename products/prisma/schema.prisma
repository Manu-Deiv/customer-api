

generator client {
  provider = "prisma-client-js"
}

datasource db {
    provider = "mongodb"
    url = env("DATABASE_URL")
}

model Product {
    id String @id @default(auto()) @map("_id") @db.ObjectId
    name String
    code String
    description String
    value Float
    created_at DateTime ? @default(now())
    updated_at DateTime ? @default(now())

    @@map("products")
}
